<?xml version="1.0" encoding="utf-8"?>
<resources>
	
	<!--刷新加载-->
	<declare-styleable name="RefreshLayout">
		<attr name="direction">
			<enum name="top" value="0" />
			<enum name="bottom" value="1" />
			<enum name="both" value="2" />
		</attr>
	</declare-styleable>
	
	<!--等高宽图片-->
	<declare-styleable name="RatioImageView">
		<attr name="ratio" type="dimension" />
	</declare-styleable>
	
	<!--九宫格图片布局-->
	<declare-styleable name="AbsNineGridLayout">
		<attr name="space" type="dimension" />
	</declare-styleable>

</resources>